    "path-to-regex": "^1.3.1",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^6.0.0-beta",
    "react-json-view": "^1.19.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "subscriptions-transport-ws": "^0.9.15",
    "typesafe-actions": "^3.0.0",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
