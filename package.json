    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-ws": "^1.0.14",
    "classnames": "^2.2.6",
    "graphql": "^14.1.1",
    "graphql-obj2arg": "^0.1.0",
    "graphql-tag": "^2.10.1",
    "immer": "^2.0.0",
