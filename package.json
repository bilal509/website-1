{
  "name": "hyper-privacy",
  "private": true,
  "scripts": {
    "build:production": "env MODE=production webpack --mode=none",
    "build:staging": "env MODE=staging webpack --mode=none",
    "build:local": "env MODE=local webpack --mode=none",
    "serve": "env MODE=local webpack -w --mode=none --info-verbosity=none",
    "start": " npm run serve & npm run start:server",
    "start:server": "cd dist && env REDIRECT_ORIGIN=false DOMAIN=127.0.0.1 ERROR_URL=http://127.0.0.1:9090/error ./server",
    "build:server": "cd src && env GO111MODULE=on CGO_ENABLED=0 GOOS=linux GOARCH=amd64 go build -o ../dist/server -a . ",
    "postinstall": "npm run build:server",
    "gqlgen": "gql-gen --config gql-gen.json"
  },
  "dependencies": {
    "@types/query-string": "^6.2.0",
    "apollo-cache-inmemory": "^1.4.3",
